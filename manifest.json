{
  "name": "CookieVis",
  "version": "1.0",
  "description": "Visualization for cookies",
  "manifest_version": 2,
  "permissions": [
  	"tabs",
  	"cookies",
  	"<all_urls>"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "logo.png"
  },
  "options_page": "vis.html"
}
